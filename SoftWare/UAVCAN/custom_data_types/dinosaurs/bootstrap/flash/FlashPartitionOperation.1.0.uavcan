#
# Flash Partition Operation
#
# index: flash partition index
# cmd:   flash partition operation command
#        0x0a: format partition
#        0x0b: load app in with image header
#        0x0c: verify partition
#

uint8 FORMATE_PARTITION_CMD = 0x0a
uint8 LOAD_PARTITION_APP_CMD = 0x0b
uint8 VERIFY_PARTITION_CMD = 0x0c

uint8 index
uint8 cmd

@sealed

